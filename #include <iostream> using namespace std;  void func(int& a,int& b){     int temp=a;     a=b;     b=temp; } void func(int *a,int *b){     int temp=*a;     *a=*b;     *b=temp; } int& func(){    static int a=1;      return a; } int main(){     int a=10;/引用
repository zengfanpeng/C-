#include <iostream>
using namespace std;

void func(int& a,int& b){
    int temp=a;
    a=b;
    b=temp;
}
void func(int *a,int *b){
    int temp=*a;
    *a=*b;
    *b=temp;
}
int& func(){
   static int a=1;

    return a;
}
int main(){
    int a=10;
   // int &b=a;

   int b=100;
    cout<<b<<endl;
    cout<<a<<endl;
    func(a,b);
    cout<<b<<endl;
    cout<<a<<endl;
    func(&a,&b);
    cout<<b<<endl;
    cout<<a<<endl;
    int &c=func();
    cout<<c<<endl;
}
